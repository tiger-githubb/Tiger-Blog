{"ast":null,"code":"export default {\n  name: 'ViewBlog',\n\n  data() {\n    return {\n      currentBlog: null\n    };\n  },\n\n  async mounted() {\n    this.currentBlog = await this.$store.state.blogPosts.filter(post => {\n      return post.blogId == this.$route.params.blogid;\n    });\n  }\n\n};","map":{"version":3,"mappings":"AAcA;EACAA,gBADA;;EAEAC;IACA;MACAC;IADA;EAGA,CANA;;EAOA;IACA;MACA;IACA,CAFA;EAGA;;AAXA","names":["name","data","currentBlog"],"sourceRoot":"src/views","sources":["ViewBlog-view.vue"],"sourcesContent":["<template>\r\n  <div class=\"post-view\" v-if=\"currentBlog\">\r\n    <div class=\"container quillWrapper\">\r\n        <h2>{{ this.currentBlog[0].blogTitle }}</h2>\r\n        <h4>Пуснат на: {{new Date(this.currentBlog[0].blogDate).toLocaleString('bg-bg')}}</h4>\r\n        <img :src=\"this.currentBlog[0].blogCoverPhoto\" alt=\"Cover Photo\">\r\n        <div class=\"post-content ql-editor\" v-html=\"this.currentBlog[0].blogHTML\">\r\n\r\n        </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'ViewBlog',\r\n    data() {\r\n        return {\r\n            currentBlog: null\r\n        };\r\n    },\r\n    async mounted() {\r\n        this.currentBlog = await this.$store.state.blogPosts.filter(post => {\r\n            return post.blogId == this.$route.params.blogid;\r\n        });\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n    .post-view {\r\n        h4 {\r\n            font-weight: 400;\r\n            font-size: 14px;\r\n            margin-bottom: 24px;\r\n        }\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}