{"ast":null,"code":"import { library } from '@fortawesome/fontawesome-svg-core';\nimport { faEnvelope } from '@fortawesome/free-solid-svg-icons';\nlibrary.add([faEnvelope]);\nimport Modal from '../components/Modal-comp.vue';\nimport Loading from '../components/Loading-comp.vue';\nimport firebase from 'firebase/app';\nimport 'firebase/auth';\nexport default {\n  name: 'ForgotPassword',\n\n  data() {\n    return {\n      email: '',\n      modalActive: false,\n      modalMessage: '',\n      loading: null\n    };\n  },\n\n  components: {\n    Modal,\n    Loading\n  },\n  methods: {\n    resetPassword() {\n      this.loading = true;\n      firebase.auth().sendPasswordResetEmail(this.email).then(() => {\n        this.modalMessage = 'Ако имате регистриран акаунт, ще получите поща.';\n        this.loading = false;\n        this.modalActive = true;\n      }).catch(err => {\n        this.modalMessage = err.message;\n        this.loading = false;\n        this.modalActive = true;\n      });\n    },\n\n    closeModal() {\n      this.modalActive = !this.modalActive;\n      this.email = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+BA;AACA;AACAA;AAEA;AACA;AAEA;AACA;AAEA;EACAC,sBADA;;EAEAC;IACA;MACAC,SADA;MAEAC,kBAFA;MAGAC,gBAHA;MAIAC;IAJA;EAMA,CATA;;EAUAC;IAAAC;IAAAC;EAAA,CAVA;EAWAC;IACAC;MACA;MAEAC,SACAC,IADA,GAEAC,sBAFA,CAEA,UAFA,EAGAC,IAHA,CAGA;QACA;QACA;QACA;MACA,CAPA,EAQAC,KARA,CAQAC;QACA;QACA;QACA;MACA,CAZA;IAaA,CAjBA;;IAkBAC;MACA;MACA;IACA;;EArBA;AAXA","names":["library","name","data","email","modalActive","modalMessage","loading","components","Modal","Loading","methods","resetPassword","firebase","auth","sendPasswordResetEmail","then","catch","err","closeModal"],"sourceRoot":"src/views","sources":["ForgotPassword-view.vue"],"sourcesContent":["<template>\r\n  <div class=\"reset-password\">\r\n    <Modal \r\n        v-if=\"modalActive\" \r\n        @close-modal=\"closeModal\" \r\n        :modalMessage=\"modalMessage\"\r\n    />\r\n    <Loading v-if=\"loading\" />\r\n    <div class=\"form-wrap\">\r\n        <form class=\"reset\">\r\n            <p class=\"login-register\">\r\n                Retour à \r\n                <router-link class=\"router-link\" :to=\"{ name: 'Login' }\">Entrée</router-link>\r\n            </p>\r\n            <h2>Réinitialiser le mot de passe</h2>\r\n            <p>Vous avez oublié votre mot de passe?Entrez votre e-mail pour réinitialiser.</p>\r\n            <div class=\"inputs\">\r\n                <div class=\"input\">\r\n                    <input type=\"text\" placeholder=\"Email\" v-model=\"email\">\r\n                    <font-awesome-icon class=\"icon\" icon=\"fa-solid fa-envelope\" />\r\n                </div>\r\n            </div>\r\n            <button @click.prevent=\"resetPassword\">Нулиране</button>\r\n            <div class=\"angle\"></div>\r\n        </form>\r\n        <div class=\"background\"></div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { library } from '@fortawesome/fontawesome-svg-core';\r\nimport { faEnvelope } from '@fortawesome/free-solid-svg-icons';\r\nlibrary.add([faEnvelope]);\r\n\r\nimport Modal from '../components/Modal-comp.vue';\r\nimport Loading from '../components/Loading-comp.vue';\r\n\r\nimport firebase from 'firebase/app';\r\nimport 'firebase/auth';\r\n\r\nexport default {\r\n    name: 'ForgotPassword',\r\n    data() {\r\n        return {\r\n            email: '',\r\n            modalActive: false,\r\n            modalMessage: '',\r\n            loading: null\r\n        }\r\n    },\r\n    components: { Modal, Loading },\r\n    methods: {\r\n        resetPassword() {\r\n            this.loading = true;\r\n            \r\n            firebase\r\n                .auth()\r\n                .sendPasswordResetEmail(this.email)\r\n                .then(() => {\r\n                    this.modalMessage = 'Ако имате регистриран акаунт, ще получите поща.';\r\n                    this.loading = false;\r\n                    this.modalActive = true;\r\n                })\r\n                .catch((err) => {\r\n                    this.modalMessage = err.message;\r\n                    this.loading = false;\r\n                    this.modalActive = true;\r\n                });\r\n        },\r\n        closeModal() {\r\n            this.modalActive = !this.modalActive;\r\n            this.email = '';\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.reset-password {\r\n    position: relative;\r\n    .form-wrap {\r\n        .reset {\r\n            h2 {\r\n                margin-bottom: 8px;\r\n            }\r\n            p {\r\n                text-align: center;\r\n                margin-bottom: 32px;\r\n            }\r\n        }\r\n    }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}